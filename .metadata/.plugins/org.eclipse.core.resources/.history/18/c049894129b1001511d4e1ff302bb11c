package com.example.test_producer_consumer;

/**
 * 1、生产者消费者
 * Producer consumer
 * 
 * @author 北飞的候鸟
 *
 */
public class Movie {

	private String pic;
	
	public synchronized void play(String str){
		this.pic = str;
	}
	
	public synchronized void watch(){
		System.out.println(this.pic);
	}
	
	public static void main(String[] args) {
		
	}
}
