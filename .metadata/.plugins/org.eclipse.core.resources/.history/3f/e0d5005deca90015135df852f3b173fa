package com.example.mtest;

/**
 * hasNext
 * next
 * @author 北飞的候鸟
 *
 */
public class MyArrayList {

	private int[] elem = {1,2,3,4,5,6,7,8,9};
	private int size = elem.length;
	private int cursor = -1;
	
	public boolean hasNext() {
		/**
		 * 判断是否存在下一个元素
		 */
		return cursor < size;
	}
	
	public int next(){
		cursor++;
		return elem[cursor];
	}
	
	public void remove(){
		
	}
	
	
	
	
	public static void main(String[] args) {
		
	}
}
