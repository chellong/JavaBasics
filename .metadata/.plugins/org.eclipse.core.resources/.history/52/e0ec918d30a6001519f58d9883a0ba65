package com.example.string;

/**
 * String (不可变) StringBuluder(线程不安全，效率高) StringBuffer (线程安全，效率低)
 * 
 * @author 北飞的候鸟
 *
 */
public class TsetStringBulider {

	public static void main(String[] args) {

		StringBuilder sb = new StringBuilder();
		long flag;

		for (int i = 0; i < 100; i++) {
			flag = sb.hashCode();
			sb = sb.append(i);
		}
	}


}
