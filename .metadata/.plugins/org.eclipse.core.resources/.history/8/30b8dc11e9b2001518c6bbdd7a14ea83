package com.example.httpserver_1;

import java.io.Closeable;
import java.io.IOException;

public class Util {
	
	public void close(Closeable ... clo){
		
		for(Closeable temp : clo){
			if(temp != null){
				try {
					temp.close();
				} catch (IOException e) {
					e.printStackTrace();
				}
			}
		}
	}

}
