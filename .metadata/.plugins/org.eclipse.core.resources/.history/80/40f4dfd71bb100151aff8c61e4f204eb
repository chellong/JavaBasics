package com.example.testsynchronized;

/**
 * 同步块{
 * 
 * }
 * 
 * 同步方法
 * @author 北飞的候鸟
 *
 */
public class Test  {
	
	public static void main(String[] args) {
		
		MyThread thread = new MyThread();
		thread.start();
	}

}


class MyThread extends Thread{
	
	
	private int num = 50;

	@Override
	public void run() {
		
		test();

	}

	private void test() {
		while (true) {
			if (num <= 0) {
				break;
			}
			System.out.println(Thread.currentThread().getName() + "  " + num--);
		}
	}
}
