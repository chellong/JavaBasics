package com.example.collection;

import java.util.LinkedList;

public class MyLinkedList {

	private Node first;
	private Node last;
	
	private int size;
	
	public void add(Object obj){
		Node n = new Node();
		if(first == null){
			n.setPrevious(null);
			n.setObj(obj);
			n.setNext(null);
			
			first = n;
			last = n;
		}else{
			n.setPrevious(last);
			n.setObj(obj);
			n.setNext(null);
			
			last.setNext(n);
			last = n;
		}
	}
	
	private void mian() {

		LinkedList list = new LinkedList<>();
	}
	
}


