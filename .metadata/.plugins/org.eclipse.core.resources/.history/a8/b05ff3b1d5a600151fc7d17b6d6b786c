package com.example.testexception;

/**
 * Throwable
 * --Error
 * --Exception {checkedException && uncheckedExcrption}
 * ----
 * @author 北飞的候鸟
 *
 */
public class Test {

	public static void main(String[] args) {
		try{
			int i = 6/0;
		}catch(ArithmeticException e){
			e.printStackTrace();
			System.out.println("除数为'0'");
		}
		
		try {
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
	}
}
