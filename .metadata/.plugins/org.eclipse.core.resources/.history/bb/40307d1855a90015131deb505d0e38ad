package com.example.collection;

/**
 * 1、自己实现map
 * 
 * @author 北飞的候鸟
 *
 */
public class MyMapOne {

	MyEntry[] arr = new MyEntry[100];
	int size;

	public void put(Object key, Object valus) {
		MyEntry e = new MyEntry(key, valus);
		arr[size++] = e;
	}

	public Object get(Object key) {
		for (int i = 0; i < size; i++) {
			if (arr[i].key.equals(key)) {
				return arr[i].obj;
			}
		}
		return null;
	}
	
	public void containsKey() {
		// TODO Auto-generated method stub

	}

	public static void main(String[] args) {

		MyMapOne mapOne = new MyMapOne();
		mapOne.put(1, 1);
		mapOne.put(2, 2);
		mapOne.put(3, 3);

		System.out.println(mapOne.get(2));
	}
}

class MyEntry {

	Object key;
	Object obj;

	public MyEntry(Object key, Object obj) {

		this.key = key;
		this.obj = obj;
	}
}