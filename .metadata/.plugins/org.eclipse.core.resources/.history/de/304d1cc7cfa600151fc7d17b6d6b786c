package com.example.testtile;

import java.io.File;

public class TreeFile {

	public void getTreeFile(File file,int level){
		for(int i = 0; i < level;i++){
			System.out.println("-");
		}
		System.out.println(file.getName());
		if(file.isDirectory()){
			File[] files = file.listFiles();
			for(File temp : files){
				getTreeFile(temp, level + 1);
			}
		}
	}
	
	File file = new File("D:/");
}
