package com.example.testproperties;

import java.io.File;
import java.io.FileOutputStream;
import java.util.Properties;

/**
 * 输出
 * 
 * @author 北飞的候鸟
 *
 */
public class TestExp {

	public static void main(String[] args) {
		
		String path = "/src/com/example/testproperties/pro.properties";
		Properties pro = new Properties();
		pro.setProperty("1","1");
		pro.setProperty("2","2");
		pro.setProperty("3","3");
		pro.setProperty("4","4");
		
		pro.store(new FileOutputStream(new File(path)),
				"数据配置");
		
	}
}
