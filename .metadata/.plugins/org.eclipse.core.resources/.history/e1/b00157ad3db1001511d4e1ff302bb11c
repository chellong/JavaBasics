			//准备接受容器
			byte[] containet = new byte[1024];
			//封装成包daragrampacket
			DatagramPacket packet = new DatagramPacket(containet,containet.length);
			//接受数据
			
			
			
			
		} catch (SocketException e) {
			e.printStackTrace();
		}
	}
}
