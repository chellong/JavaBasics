package com.example.httpserver_0;

import java.io.IOException;
import java.net.ServerSocket;

public class Server {
	
	public static void main(String[] args) {
		
	}
	
	public void start(){
		try {
			ServerSocket serverSocket = new ServerSocket(8080);
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
	

}
